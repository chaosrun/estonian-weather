@startuml backend-classes

class Forecast {
    id: int
    date: Date
    updatedAt: Date
}

class ForecastPeriod {
    id: int
    phenomenon: String
    tempMin: int
    tempMax: int
    text: String
    sea: String
    peipsi: String
}

class Place {
    id: int
    name: String
    phenomenon: String
    tempMin: int
    tempMax: int
}

class Wind {
    id: int
    name: String
    direction: String
    speedMin: int
    speedMax: int
    gust: int
}

Forecast "1" - "1" ForecastPeriod : night >
Forecast "1" - "1" ForecastPeriod : day >
ForecastPeriod "1  " -- "n" Place : place >
ForecastPeriod "1 " -- "n  " Wind : wind >

@enduml
